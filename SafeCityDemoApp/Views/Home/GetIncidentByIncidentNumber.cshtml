@model Incident
@{
    ViewBag.Title = "GetIncidentByIncidentNumber";
}

<h2>Incident# @Model.Number</h2>

<dl class="dl-horizontal">
    <dt>Id</dt>    <dd>@Model.Id</dd>
    <dt>Number</dt>    <dd>@Model.Number</dd>
    <dt>Caller Number</dt>    <dd>@Model.CallerNumber</dd>

    <dt>Caller Name</dt>    <dd>@Model.CallerName</dd>

</dl>

<a href="@Url.Action("CreateIncident")">Create New</a>